{
  "name": "avm1-decompiler",
  "version": "0.0.1",
  "description": "AVM1 decompiler",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "keywords": [
    "swf",
    "parser"
  ],
  "license": "MIT",
  "scripts": {
    "prepare": "gulp all:tsconfig.json && gulp :tslint.json && gulp dist",
    "test": "gulp :lint && gulp test",
    "prestart": "gulp main:build",
    "start": "node build/main/main/main.js",
    "prepublishOnly": "npm test",
    "lint": "gulp :lint:fix"
  },
  "dependencies": {
    "incident": "^3.1.1",
    "semantic-types": "^0.1.1",
    "avm1-parser": "^0.0.2",
    "avm1-tree": "^0.0.16"
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^5.2.0",
    "@types/node": "^10.0.0",
    "bson": "^2.0.5",
    "chai": "^4.1.2",
    "esm": "^3.0.28",
    "gulp": "^4.0.0",
    "gulp-cli": "^2.0.1",
    "kryo": "^0.7.0",
    "minimist": "^1.2.0",
    "ts-node": "^6.0.2",
    "tslint": "^5.9.1",
    "turbo-gulp": "^0.17.0",
    "typescript": "^2.8.3"
  },
  "nyc": {
    "include": [
      "build/test/lib/**/*.js",
      "build/test/lib/**/*.mjs"
    ],
    "reporter": [
      "text",
      "html"
    ],
    "extension": [
      ".mjs"
    ]
  },
  "esm": {
    "mode": "strict"
  }
}
