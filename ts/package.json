{
  "name": "avm1-decompiler",
  "version": "0.0.1",
  "description": "AVM1 decompiler",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "private": true,
  "keywords": [
    "swf",
    "parser"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gulp lib:build",
    "watch": "gulp lib:watch",
    "test": "gulp test",
    "lint": "gulp lint",
    "format": "gulp format",
    "typedoc": "gulp lib:typedoc",
    "dist": "gulp lib:dist",
    "prepare": "gulp all:tsconfig.json && gulp tslint.json && gulp dist",
    "pretest": "gulp lint",
    "prepublishOnly": "echo \"Use \\`gulp lib:publish\\`\" && exit 1",
    "prestart": "gulp main:build",
    "start": "node build/main/main/main.js"
  },
  "dependencies": {
    "avm1-parser": "^0.10.0",
    "avm1-types": "^0.10.0",
    "incident": "^3.2.0",
    "semantic-types": "^0.1.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.9",
    "@types/gulp": "^4.0.6",
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.4",
    "chai": "^4.2.0",
    "esm": "^3.2.25",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.2.0",
    "kryo": "^0.8.1",
    "minimist": "^1.2.3",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "turbo-gulp": "^0.22.1",
    "typescript": "^3.8.2"
  },
  "esm": {
    "mode": "strict"
  },
  "c88": {
    "match": [
      "build/test/lib/*.{js,mjs}",
      "build/test/lib/**/*.{js,mjs}"
    ]
  }
}
